<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.EmergencyContactModuleActivity">

    <TextView
        android:id="@+id/EG_Heading_txt2"
        android:layout_width="338dp"
        android:layout_height="53dp"
        android:layout_marginTop="36dp"
        android:gravity="center"
        android:text="@string/eyeGuardianPageTxtView"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/currentNameTxt"
        android:layout_width="258dp"
        android:layout_height="26dp"
        android:layout_marginTop="109dp"
        android:text="TextView"
        android:minHeight="50dp"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EG_Heading_txt2" />

    <TextView
        android:id="@+id/currentEmergNoTxt"
        android:layout_width="257dp"
        android:layout_height="120dp"
        android:layout_marginTop="24dp"
        android:minHeight="50dp"
        android:text="TextView"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/currentNameTxt" />

    <TextView
        android:id="@+id/currentEmergNoTxt2"
        android:layout_width="257dp"
        android:layout_height="77dp"
        android:layout_marginTop="36dp"
        android:minHeight="50dp"
        android:text="TextView"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/currentEmergNoTxt" />

    <Button
        android:id="@+id/addNewEmergDetailBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="90dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="90dp"
        android:backgroundTint="#FEFEFE"
        android:minHeight="50dp"
        android:text="@string/addNewEmergDetailBtnTxt"
        app:cornerRadius="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/currentEmergNoTxt2"
        app:strokeColor="@color/black"
        app:strokeWidth="1dp" />
</androidx.constraintlayout.widget.ConstraintLayout>